{
  "name": "react-timeago",
  "version": "7.2.0",
  "description": "A simple Time-Ago component for ReactJs",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "scripts": {
    "babel-es6": "ES6=true babel src/ --out-dir es6/",
    "postbabel-es6": "node ./scripts/gen-types.js -i src -o es6/",
    "babel": "babel src/ --out-dir lib/",
    "postbabel": "node ./scripts/gen-types.js -i src -o lib/",
    "build": "npm run babel && npm run babel-es6 && npm run example",
    "example": "rollup -c ./rollup.config.mjs",
    "prepublish": "npm run build",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naman34/react-timeago.git"
  },
  "keywords": [
    "React",
    "ReactJS",
    "Time",
    "Ago",
    "TimeAgo",
    "ender"
  ],
  "author": "Naman Goel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nmn/react-timeago/issues"
  },
  "homepage": "https://github.com/nmn/react-timeago",
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-flow": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-replace": "5.0.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "14.0.0",
    "babel-plugin-syntax-hermes-parser": "^0.15.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ft-flow": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "flow-api-translator": "^0.15.1",
    "flow-bin": "^0.216.1",
    "hermes-eslint": "^0.15.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "^2.8.8",
    "prettier-plugin-hermes-parser": "^0.15.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "latest",
    "yargs": "^17.7.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-hermes-parser"
    ],
    "printWidth": 80,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  }
}
